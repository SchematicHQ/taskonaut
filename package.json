{
  "name": "@schematichq/taskonaut",
  "type": "module",
  "version": "1.10.5",
  "description": "Interactive CLI tool for exec into AWS ECS tasks (containers), rollback services to previous task definition revisions, and clean up unused task definitions - from [SchematicHQ](https://schematichq.com)",
  "main": "index.js",
  "bin": {
    "taskonaut": "./index.js"
  },
  "scripts": {
    "check-latest": "npm-check-updates",
    "force-latest": "npm-check-updates -u && npm install",
    "commit": "git-cz",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write '**/*.{js,json,md}'",
    "start": "node index.js",
    "dev": "node --watch index.js",
    "semantic-release": "semantic-release",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SchematicHQ/taskonaut.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "aws",
    "ecs",
    "exec",
    "cli",
    "task",
    "container",
    "docker",
    "devops",
    "aws-cli",
    "ecs-exec"
  ],
  "author": "SchematicHQ",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "README.md"
  ],
  "bugs": {
    "url": "https://github.com/SchematicHQ/taskonaut/issues"
  },
  "homepage": "https://github.com/SchematicHQ/taskonaut#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-ecs": "^3.943.0",
    "@aws-sdk/credential-providers": "^3.948.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "conf": "^15.0.2",
    "dotenv": "^17.2.3",
    "figlet": "^1.9.4",
    "gradient-string": "^3.0.0",
    "ini": "^6.0.0",
    "inquirer": "^13.0.2",
    "npm-check-updates": "^19.1.2",
    "ora": "^9.0.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@jest/globals": "^30.2.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^13.1.2",
    "commitizen": "^4.3.1",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "semantic-release": "^25.0.2"
  }
}
