{
  "name": "@schematichq/taskonaut",
  "type": "module",
  "version": "0.0.1-development",
  "description": "Interactive CLI tool for exec into AWS ECS tasks (containers) - from [SchematicHQ](https://schematichq.com)",
  "main": "index.js",
  "bin": {
    "taskonaut": "./index.js"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write '**/*.{js,json,md}'",
    "start": "node index.js",
    "dev": "node --watch index.js",
    "semantic-release": "semantic-release",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SchematicHQ/taskonaut.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "aws",
    "ecs",
    "exec",
    "cli",
    "task",
    "container",
    "docker",
    "devops",
    "aws-cli",
    "ecs-exec"
  ],
  "author": "SchematicHQ",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "README.md"
  ],
  "bugs": {
    "url": "https://github.com/SchematicHQ/taskonaut/issues"
  },
  "homepage": "https://github.com/SchematicHQ/taskonaut#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-ecs": "^3.817.0",
    "@aws-sdk/credential-providers": "^3.817.0",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "conf": "^13.1.0",
    "dotenv": "^16.5.0",
    "figlet": "^1.8.1",
    "gradient-string": "^3.0.0",
    "ini": "^5.0.0",
    "inquirer": "^12.6.3",
    "ora": "^8.2.0",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@jest/globals": "^29.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.5"
  }
}