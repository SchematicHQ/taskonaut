{
  "name": "@schematichq/taskonaut",
  "type": "module",
  "version": "0.0.1-development",
  "description": "Interactive CLI tool for exec into AWS ECS tasks (containers) - from [SchematicHQ](https://schematichq.com)",
  "main": "index.js",
  "bin": {
    "taskonaut": "./index.js"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write '**/*.{js,json,md}'",
    "start": "node index.js",
    "dev": "node --watch index.js",
    "semantic-release": "semantic-release",
    "postinstall": "chmod +x index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SchematicHQ/taskonaut.git"
  },
  "keywords": [
    "aws",
    "ecs",
    "exec",
    "cli",
    "task",
    "container",
    "docker",
    "devops",
    "aws-cli",
    "ecs-exec"
  ],
  "author": "SchematicHQ",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "README.md"
  ],
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": true
      }
    ]
  },
  "bugs": {
    "url": "https://github.com/SchematicHQ/taskonaut/issues"
  },
  "homepage": "https://github.com/SchematicHQ/taskonaut#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-ecs": "^3.714.0",
    "@aws-sdk/credential-providers": "^3.715.0",
    "chalk": "^5.4.0",
    "commander": "^12.1.0",
    "conf": "^13.1.0",
    "dotenv": "^16.4.7",
    "figlet": "^1.8.0",
    "gradient-string": "^3.0.0",
    "ini": "^5.0.0",
    "inquirer": "^12.2.0",
    "ora": "^8.1.1",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.0"
  }
}
